     <div class="container">

        <form method="post" action="send">
             <div class="card  border-primary bg-danger">
               <div class="col-md-4 offset-md-4">
                <div class="card-body">
                    <h3 class="card-title">Informations Prerequises</h3>
                    <p class="card-text">Les Informations rentr√© sous dessous nous permettrons de constituer votre dossier de reclamation</p>
                  </div>
             </div>
                <div class="row">
                     <div class="col-md-4 offset-md-8">
                        <label for="disabledTextInput">Numero de Police</label>
                        <div >
                            <input type="text"  name="numero_police" id="disabledTextInput" class="form-control" value =" <%= numero_police %>" disabled>
                        </div>
                    </div>
                </div>
                 <div class="row">
                     <div class="col-md-4 offset-md-8">
                        <label for="disabledTextInput">Votre Assurance</label>
                        <div >
                            <input type="text" name="assurance" id="disabledTextInput" class="form-control" placeholder="Assurance" disabled>
                        </div>
                    </div>
                </div>
                <br><br><br>

              <div class="row">
                <div class="col-sm">
                    <div class="form-group">
                        <div>
                            <label>Type de sinistre</label>
                            <select name="type_sinistre" class="select2 form-control custom-select">
                                <optgroup>
                                    <option value="SSA">Sinistre Sans Adversaire</option>
                                    <option value="SAA">Sinistre Avec Adversaire</option>
                                </optgroup> 
                            </select>
                        </div>
                    </div>
                    
                  
                </div>
                <div class="col-sm">

                     <div class="form-group">
                        <div>
                            <label>Categorie du sinistre</label>
                            <select name="cat_sinistre" class="select2 form-control custom-select">
                                <option>Selectionner</option>
                                <optgroup>
                                    <option value="DC">DOMMAGE CORPOREL</option>
                                    <option value="DM">DOMMAGE MATERIEL</option>
                                    <option value="DM/DC">DOMMAGE MATERIEL ET CORPOREL</option>
                                </optgroup> 
                            </select>
                        </div>
                    </div>
                </div>
               
              </div>
           

            <div class="row">
                <div class="col-sm">
                    <div class="form-group">
                         <label >Date du Sinistre</label>
                    <div class="input-group">
                     <input type="text" name="date_sinistre" class="form-control" id="datepicker-autoclose" placeholder="mm/dd/yyyy">
                        <div class="input-group-append">
                         <span class="input-group-text"><i class="fa fa-calendar"></i></span>
                         </div>
                    </div>
                      
                         </div>
                    </div>
                    
                  
                
                <div class="col-sm">

                    <div class="form-group">
                         
                        <label>Importer Votre Permis  </label>
                                    <div>
                                <div class="custom-file">
                                    <input type="file" name="permis" class="custom-file-input" id="validatedCustomFile">
                                    <label class="custom-file-label" for="validatedCustomFile">Attestation...</label>
                                    <div class="invalid-feedback">Example invalid custom file feedback</div>
                                </div>
                            </div>
                         </div>
                </div>
                <div class="col-sm">
                  <div class="form-group">  
                        <label>Importer Votre carte grise </label>
                                    <div>
                                <div class="custom-file">
                                    <input type="file" name="carte_grise" class="custom-file-input" id="validatedCustomFile" >
                                    <label class="custom-file-label" for="validatedCustomFile"></label>
                                    <div class="invalid-feedback">max:4 MB</div>
                                </div>
                            </div>
                         </div>
                </div>
             </div>

              <div class="row">
                <div class="col-sm">
                    <div class="form-group">
                         
                        <label>Importer Votre derniere visite technique </label>
                                    <div>
                                <div class="custom-file">
                                    <input type="file" name="visite_tech" class="custom-file-input" id="validatedCustomFile">
                                    <label class="custom-file-label" for="validatedCustomFile">Attestation...</label>
                                    <div class="invalid-feedback">Example invalid custom file feedback</div>
                                </div>
                            </div>
                         </div>
                    </div>
                    
                  
                
                <div class="col-sm">

                    <div class="form-group">
                         
                        <label>Importer Votre element de production  </label>
                                    <div>
                                <div class="custom-file">
                                    <input type="file" name="element_prod" class="custom-file-input" id="validatedCustomFile" >
                                    <label class="custom-file-label" for="validatedCustomFile">Attestation...</label>
                                    <div class="invalid-feedback">Example invalid custom file feedback</div>
                                </div>
                            </div>
                         </div>
                </div>
                <div class="col-sm">

                      <label>Importer Votre Attestation </label>
                    <div>
                        <div class="custom-file">
                            <input type="file" name="attestation"  class="custom-file-input" id="validatedCustomFile">
                            <label class="custom-file-label" for="validatedCustomFile">Attestation...</label>
                            <div class="invalid-feedback">Example invalid custom file feedback</div>
                        </div>
                     </div>
                 
                </div>
             </div>

             <div class="row">
                  <div class="col-sm">
                     <label>Choissisez votre sinistre</label>
                            <select name="sinistre" class="select2 form-control custom-select">
                                <option>Selectionner</option>
                                <optgroup>
                                    <option value="VOL">Vol</option>
                                    <option value="BDG">Bris de Glace</option>
                                    <option value="I">Incendie</option>
                                    <option value="D">Dommage</option>
                                </optgroup> 
                            </select>

                  </div>
                        
            </div>

            <br> <p>(*)Champs Obligatoire</p>
            <hr><br>
            
             <div class ="row">
                <div class="border-top">
                    <div>
                        <button type="submit" class="btn btn-primary">Suivant</button>
                    </div>
                </div>
             </div>
</div>

    </form>

 

